        "remixicon": "^3.5.0"
    },
    "devDependencies": {
        "@codeceptjs/configure": "^1.0.4",
        "@codeceptjs/examples": "^1.2.4",
        "@codeceptjs/ui": "^1.2.4",
        "@playwright/test": "^1.52.0",
        "@types/node": "^22.15.30",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "codeceptjs": "^3.7.3",
        "eslint": "^8.53.0",
